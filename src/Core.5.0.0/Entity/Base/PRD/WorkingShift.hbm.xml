<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
    <class name="com.Sconit.Entity.PRD.WorkingShift, com.Sconit.Entity" table="PRD_WorkingShift" lazy="false">
        <id name="Id" column="Id" type="Int32" unsaved-value="0" >
            <generator class="identity" />
        </id>
        <many-to-one name="Shift" column="Shift" class="com.Sconit.Entity.PRD.ShiftMaster, com.Sconit.Entity" cascade="none" />
        <!--<property name="Shift" column="Shift" type="String" length="50"/>-->
        <property name="WorkingCalendar" column="WorkingCalendar" type="Int32"/>
        <property name="CreateUserId" column="CreateUser" type="Int32" update="false"/>
        <property name="CreateUserName" column="CreateUserNm" type="String" length="100" update="false"/>
        <property name="CreateDate" column="CreateDate" type="DateTime" update="false"/>
        <property name="LastModifyUserId" column="LastModifyUser" type="Int32"/>
        <property name="LastModifyUserName" column="LastModifyUserNm" type="String" length="100"/>
        <property name="LastModifyDate" column="LastModifyDate" type="DateTime"/>
    </class>
</hibernate-mapping>
