  <sql-query name="USP_Busi_GetActiveFlowDet">
    exec USP_Busi_GetActiveFlowDet ?
    <!--
      0	 det.Id, 
      1	 mstr.Code AS Flow, 
      2	 mstr.PartyFrom, 
      3	 mstr.PartyTo, 
      4	 str.Strategy, 
      5	 str.LeadTime, 
      6	 str.EmLeadTime, 
      7	 str.TimeUnit, 
      8	 str.WeekInterval, 
      9	 str.WinTime1, 
      10	 str.WinTime2,
      11	 str.WinTime3, 
      12	 str.WinTime4, 
      13	 str.WinTime5, 
      14	 str.WinTime6, 
      15	 str.WinTime7, 
      16	 str.NextOrderTime, 
      17	 str.NextWinTime, 
      18	 mstr.Type, 
      19	 det.Item, 
      20	 det.Uom, 
      21	 det.BaseUom, 
      22	 det.UC,
      23	 CASE WHEN isnull(det.LocFrom, '') = '' THEN mstr.LocFrom ELSE det.LocFrom END AS LocFrom, 
      24	 CASE WHEN isnull(det.LocTo, '') = '' THEN mstr.LocTo ELSE det.LocTo END AS LocTo, 
      25	 det.SafeStock, 
      26	 det.MaxStock, 
      27	 det.MinLotSize, 
      28	 det.OrderLotSize, 
      29	 det.RoundUpOpt, 
      30	 det.MrpWeight, 
      31	 det.MrpTotal,
      32	 det.MrpTotalAdj, 
      33	 det.ExtraDmdSource,
      34   mstr.ExtraDmdSource
  -->
  </sql-query>

  <sql-query name="USP_Busi_GetDeferredFeedOrderBomDet">
    <return alias="bfOrderBomDet" class="com.Sconit.Entity.ORD.OrderBomDetail, com.Sconit.Entity"/>
