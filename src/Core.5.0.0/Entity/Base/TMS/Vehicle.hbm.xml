        <property name="MobilePhone" column="MobilePhone" type="String" length="50"/>
        <property name="VIN" column="VIN" type="String" length="50"/>	
		<property name="EngineNo" column="EngineNo" type="String" length="50"/>	
		<property name="Address" column="Address" type="String" length="255"/>	
		<property name="Fax" column="Fax" type="String" length="50"/>	
		<property name="Driver" column="Driver" type="String" length="50"/>	
		<property name="Tonnage" column="Tonnage" type="String" length="50"/>	
		<property name="CreateUserName" column="CreateUserNm" type="String" length="255" update="false"/>	
		<property name="CreateUserId" column="CreateUser" type="Int32" update="false"/>	
		<property name="CreateDate" column="CreateDate" type="DateTime" update="false"/>	
		<property name="LastModifyUserName" column="LastModifyUserNm" type="String" length="255"/>	
		<property name="LastModifyUserId" column="LastModifyUser" type="Int32"/>	
