@using com.Sconit.Entity.MD
@using com.Sconit.Entity.SYS
@using com.Sconit.Web.Models.SearchModels.FMS
<fieldset>
    @using (Html.BeginForm("List", "FacilityCategory"))
    {   
        <div class="search">
            <div class="search-label">
                <label for="FacilityCategory_Code">
                    @Resources.FMS.FacilityCategory.FacilityCategory_Code
                </label>
            </div>
            <div class="search-field">
                @Html.TextBox("Code", TempData["FacilityCategorySearchModel"] != null ? ((FacilityCategorySearchModel)TempData["FacilityCategorySearchModel"]).Code : string.Empty)
            </div>
            <div class="search-label">
                <label for="FacilityCategory_Address">
                    @Resources.FMS.FacilityCategory.FacilityCategory_Description
                </label>
            </div>
            <div class="search-field">
                @Html.TextBox("Description", TempData["FacilityCategorySearchModel"] != null ? ((FacilityCategorySearchModel)TempData["FacilityCategorySearchModel"]).Description : string.Empty)
            </div>
            <div class="search-label">
                <label for="FacilityCategory_ChargePerson">
                    @Resources.FMS.FacilityCategory.FacilityCategory_ChargePerson
                </label>
            </div>
            <div class="search-field">
                @Html.TextBox("ChargePersonName", TempData["FacilityCategorySearchModel"] != null ? ((FacilityCategorySearchModel)TempData["FacilityCategorySearchModel"]).ChargePersonName : string.Empty)
            </div>
        </div>
        <div class="search-footer">
            @Html.Hidden("isFromList", true)
            @Html.Button(Resources.SYS.Global.Button_Search, "Url_FacilityCategory_View", new Dictionary<string, string>()
	{
	    {"type", "submit"},
	    {"id", "btnSearch"}
	})
            @Html.Button(Resources.SYS.Global.Button_New, "Url_FacilityCategory_Edit", new Dictionary<string, string>()
	{
	    {"type", "button"},
	    {"id", "btnNew"},
        {"onclick","location.href='"+@Url.Content("~/FacilityCategory/New")+"'"}
	})
        </div>

    }
</fieldset>
