@model com.Sconit.Entity.ORD.OrderMaster
@{
    Html.RenderAction("_SiteMapPath", "Common", new { MenuContent = "Url_OrderMstr_Production_SeqImport" });

}
@using (Html.BeginForm("ImportSeq", "ProductionOrder"))
{
    <fieldset>
        <div class="editor">
            <div class="editor-label-2">
                <label>
                    @Resources.ORD.OrderMaster.OrderMaster_ExternalSeqImport_DateTime
                </label>
            </div>
            <div class="editor-field-2">
               @(Html.Telerik().DatePicker().Name("StartDate").HtmlAttributes(new { style = "margin-bottom: 1.3em;width:200px" }))
            </div>
        </div>
        <div class="editor-footer">
            @Html.Button(Resources.SYS.Global.Button_Import, "Url_OrderMstr_Production_SeqImport", new Dictionary<string, string>()
	{
	    {"type", "submit"},
	    {"id", "btnSeqImport"},
        {"needconfirm","true"}
	})
        </div>
    </fieldset>
   
}
