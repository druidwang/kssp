@using com.Sconit.Entity.SYS
@model com.Sconit.Entity.INV.ContainerDetail
@{
    Html.RenderAction("_SiteMapPath", "Common", new { MenuContent = "Url_Container_View" });
}
@using (Html.BeginForm())
{
    <fieldset>
        <legend>@Resources.MD.Container.Container_Edit_Title</legend>
        <div class="editor">
            <div class="editor-label-2">
                容器编号
            </div>
            <div class="editor-field-2">
                @Html.DisplayFor(model => model.ContainerId)
            </div>
            <div class="editor-label-2">
                容器类型
            </div>
            <div class="editor-field-2">
                @Html.DisplayFor(model => model.Container)
            </div>
            <div class="editor-label-2">
              容器描述
            </div>
            <div class="editor-field-2">
                @Html.DisplayFor(model => model.ContainerDescription)
            </div>
            <div class="editor-label-2">
               容量
            </div>
            <div class="editor-field-2">
                @Html.EditorFor(model => model.ContainerQty)
            </div>
        </div>
        <div class="editor-footer">
            @Html.Button(Resources.SYS.Global.Button_Print, "Url_ContainerDetail_View", new Dictionary<string, string>()
	{
	    {"type", "submit"},
	    {"id", "btnPrint"}
	})
            <button type="button" id="btnBack" onclick="location.href='@Url.Content("~/Container/List")'">
                @Resources.SYS.Global.Button_Back</button>
        </div>
    </fieldset>
}
