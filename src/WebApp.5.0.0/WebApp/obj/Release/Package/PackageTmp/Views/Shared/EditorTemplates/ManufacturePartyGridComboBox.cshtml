@(Html.Telerik().ComboBox()
    .Name("ManufactureParty")
                 .HiddenInputHtmlAttributes(new { id = "ManufactureParty" })
     // .Name(ViewData.TemplateInfo.GetFullHtmlFieldName(""))
              .DataBinding(binding => binding.Ajax().Select("_AjaxLoadingManufactureParty", "Common").Cache(true))
      .AutoFill(true)
      .HighlightFirstMatch(true)
          .ClientEvents(events => events.OnDataBinding("onManufacturePartyDataBinding")
                      .OnChange("onManufacturePartyChange")
      )
)
