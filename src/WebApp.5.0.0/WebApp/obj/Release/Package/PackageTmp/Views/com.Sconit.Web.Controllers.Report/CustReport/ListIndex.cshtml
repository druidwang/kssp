            _BomComboBox: $("#_BomComboBox").val(),
            _CodeMasterComboBox: $("#_CodeMasterComboBox").val(),
            _CodeMasterDropDownList: $("#_CodeMasterDropDownList").val(),
            Text2: $("#Text2").val(),
            _CommonDropDownList: $("#_CommonDropDownList").val(),
            Text3: $("#Text3").val(),
            _CostCenterComboBox: $("#_CostCenterComboBox").val(),
            _CurrencyDropDownList: $("#_CurrencyDropDownList").val(),
            _CustomerComboBox: $("#_CustomerComboBox").val(),
            _DateIndexComboBox: $("#_DateIndexComboBox").val(),
            _DefectCodeComboBox: $("#_DefectCodeComboBox").val(),
            _DefectCodeDropDownList: $("#_DefectCodeDropDownList").val(),
            _FailCodeComboBox: $("#_FailCodeComboBox").val(),
            _FiShiftComboBox: $("#_FiShiftComboBox").val(),
            _FlowComboBox: $("#_FlowComboBox").val(),
            _FlowItemComboBox: $("#_FlowItemComboBox").val(),
            Text4: $("#Text4").val(),
            _HuToComboBox: $("#_HuToComboBox").val(),
            _InspectComboBox: $("#_InspectComboBox").val(),
            _IslandComboBox: $("#_IslandComboBox").val(),
            Text5: $("#Text5").val(),
            _ItemCategoryDropDownList: $("#_ItemCategoryDropDownList").val(),
            _ItemComboBox: $("#_ItemComboBox").val(),
            _ItemPackageComboBox: $("#_ItemPackageComboBox").val(),
            _ItemPackageDropDownList: $("#_ItemPackageDropDownList").val(),
            _LocationAreaDropDownList: $("#_LocationAreaDropDownList").val(),
            _LocationBinComboBox: $("#_LocationBinComboBox").val(),
            _LocationBinDropDownList: $("#_LocationBinDropDownList").val(),
            _LocationComboBox: $("#_LocationComboBox").val(),
            _MachineComboBox: $("#_MachineComboBox").val(),
            _ManufacturePartyComboBox: $("#_ManufacturePartyComboBox").val(),
            _ManufacturePartyDropDownList: $("#_ManufacturePartyDropDownList").val(),
            _MoveTypeDropDownList: $("#_MoveTypeDropDownList").val(),
            _MrpPlanMasterComboBox: $("#_MrpPlanMasterComboBox").val(),
            _MrpSnapMasterComboBox: $("#_MrpSnapMasterComboBox").val(),
            _OrderMasterPartyFromComboBox: $("#_OrderMasterPartyFromComboBox").val(),
            _OrderMasterPartyToComboBox: $("#_OrderMasterPartyToComboBox").val(),
