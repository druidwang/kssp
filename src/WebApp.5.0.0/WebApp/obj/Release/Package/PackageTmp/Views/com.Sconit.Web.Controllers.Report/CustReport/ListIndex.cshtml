                  @{
                      Html.RenderAction("_UomDropDownList", "Common", new
                      {
                          controlName = "_UomDropDownList",
                          includeBlankOption = true
                      });
                  }
             </div>
        }
        @if (ViewBag._WorkingCalendarFlowComboBox)
        {
             <div class="editor-label-2">
                 <label>
                  @ViewBag._WorkingCalendarFlowComboBoxText
                </label>
             </div>
             <div class="editor-field-2">
                  @{
                      Html.RenderAction("_WorkingCalendarFlowComboBox", "Common", new
                      {
                          controlName = "_WorkingCalendarFlowComboBox",
                          includeBlankOption = true
                      });
                  }
             </div>
        }
        @if (ViewBag.StartDate)
        {
            <div class="search-label">
                <label>
                    @ViewBag.StartDateText
                </label>
            </div>
            <div class="search-field">
                @(Html.Telerik().DateTimePicker().Format("yyyy-MM-dd HH:mm").Name("StartDate")
                .Value(System.DateTime.Today)
                .HtmlAttributes(new { style = "margin-bottom: 1.3em;width:200px" }))
            </div>
        }
        @if (ViewBag.EndDate)
        {
            <div class="search-label">
                <label>
                    @ViewBag.EndDateText
                </label>
            </div>
            <div class="search-field">
                @(Html.Telerik().DateTimePicker().Format("yyyy-MM-dd HH:mm").Name("EndDate")
                .Value(System.DateTime.Today)
                .HtmlAttributes(new { style = "margin-bottom: 1.3em;width:200px" }))
            </div>
        }

    </div>
    <div class="search-footer">
        @Html.Button(Resources.SYS.Global.Button_Search, "Url_CustReport_ReportMenu", new Dictionary<string, string>()
	        {
	            {"type", "button"},
	            {"id", "btnSearch"},
                {"needconfirm","false"},
                {"onclick","onSearch()"},
	        })
        <button type="button" id="btnExport">@Resources.SYS.Global.Button_Export</button>
    </div>
    
}
</fieldset>
<div id="fdOrderDetail">
</div>
<script type="text/javascript">
    $('#btnExport').click(function () {
        $('form').submit();
    });
</script>
