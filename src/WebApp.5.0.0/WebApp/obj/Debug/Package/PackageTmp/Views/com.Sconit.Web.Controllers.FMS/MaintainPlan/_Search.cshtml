@using com.Sconit.Entity.MD
@using com.Sconit.Entity.SYS
@using com.Sconit.Web.Models.SearchModels.FMS
<fieldset>
    @using (Html.BeginForm("List", "MaintainPlan"))
    {   
        <div class="search">
            <div class="search-label">
                <label for="MaintainPlan_Code">
                    @Resources.FMS.MaintainPlan.MaintainPlan_Code
                </label>
            </div>
            <div class="search-field">
                @Html.TextBox("Code", TempData["MaintainPlanSearchModel"] != null ? ((MaintainPlanSearchModel)TempData["MaintainPlanSearchModel"]).Code : string.Empty)
            </div>
            <div class="search-label">
                <label for="MaintainPlan_Address">
                    @Resources.FMS.MaintainPlan.MaintainPlan_Description
                </label>
            </div>
            <div class="search-field">
                @Html.TextBox("Description", TempData["MaintainPlanSearchModel"] != null ? ((MaintainPlanSearchModel)TempData["MaintainPlanSearchModel"]).Description : string.Empty)
            </div>
        </div>
        <div class="search-footer">
            @Html.Hidden("isFromList", true)
            @Html.Button(Resources.SYS.Global.Button_Search, "Url_MaintainPlan_View", new Dictionary<string, string>()
	{
	    {"type", "submit"},
	    {"id", "btnSearch"}
	})
            @Html.Button(Resources.SYS.Global.Button_New, "Url_MaintainPlan_Edit", new Dictionary<string, string>()
	{
	    {"type", "button"},
	    {"id", "btnNew"},
        {"onclick","location.href='"+@Url.Content("~/MaintainPlan/New")+"'"}
	})
        </div>

    }
</fieldset>
