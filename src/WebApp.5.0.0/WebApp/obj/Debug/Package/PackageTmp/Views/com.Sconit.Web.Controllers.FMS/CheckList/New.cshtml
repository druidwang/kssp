@using com.Sconit.Entity.SYS
@model com.Sconit.Entity.FMS.CheckListMaster
@{
    Html.RenderAction("_SiteMapPath", "Common", new { MenuContent = "Url_CheckList_View" });
}
@using (Html.BeginForm())
{
    <fieldset>
        <legend>@Resources.FMS.CheckList.CheckList_New_Title</legend>
        <div class="editor">
            <div class="editor-label-2">
                @Html.LabelFor(model => model.Code)
            </div>
            <div class="editor-field-2">
                @Html.EditorFor(model => model.Code)
                @Html.ValidationMessageFor(model => model.Code)
            </div>
            <div class="editor-label-2">
                @Html.LabelFor(model => model.Name)
            </div>
            <div class="editor-field-2">
                @Html.EditorFor(model => model.Name)
            </div>
            <div class="editor-label-2">
                @Html.LabelFor(model => model.Region)
            </div>
            <div class="editor-field-2">
                 @Html.EditorFor(model => model.Region)
            </div>
            <div class="editor-label-2">
                @Html.LabelFor(model => model.Description)
            </div>
            <div class="editor-field-2">
                @Html.EditorFor(model => model.Description)
            </div>
        </div>
        <div class="editor-footer">
            <button type="submit" id="btnNew">
                @Resources.SYS.Global.Button_New</button>
            <button type="button" id="btnBack" onclick="location.href='@Url.Content("~/CheckList/List")'">
                @Resources.SYS.Global.Button_Back</button>
        </div>
    </fieldset>
}