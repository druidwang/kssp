    <root>
      <level value="ERROR"/>
      <appender-ref ref="DebugLog" />
    </root>

    <logger name="NHibernate.SQL" additivity="false">
      <level value="DEBUG"/>
      <appender-ref ref="DebugSQL"/>
    </logger>

    <logger name="Log.WebAppError" additivity="false">
      <level value="ERROR"/>
