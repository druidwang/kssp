                @Resources.SYS.Global.Button_Print</button>
            <button type="button" id="btnExport" onclick="Export()">
                @Resources.SYS.Global.Button_Export</button>
            <button type="button" id="btnBack" onclick="location.href='@Url.Content("~/VehicleInFactory/List")'">
                @Resources.SYS.Global.Button_Back</button>
        </div>
    }
</fieldset>
@{
    Html.RenderAction("_ViewVehicleInFactoryDetailList", "VehicleInFactory", new { OrderNo = Model.OrderNo });
