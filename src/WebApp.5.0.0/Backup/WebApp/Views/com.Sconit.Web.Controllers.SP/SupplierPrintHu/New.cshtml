@{
    Html.RenderAction("_SiteMapPath", "Common", new { MenuContent = "Url_Supplier_Print_ADD" });
}
@{ Html.Telerik().TabStrip()
        .Name("TabStrip")
        .Items(tabstrip =>
        {
            tabstrip.Add()
               .Text(Resources.INV.Hu.Hu_Tab_FlowMaster)
               .LoadContentFrom("FlowMaster", "SupplierPrintHu")
               .HtmlAttributes(new { onclick = "CleanTabMessage()" });
               
            //tabstrip.Add()
            //    .Text(Resources.INV.Hu.Hu_Tab_OrderMaster)
            //    .LoadContentFrom("OrderMaster", "SupplierPrintHu")
            //    .HtmlAttributes(new { onclick = "CleanTabMessage()" });
            tabstrip.Add()
               .Text(Resources.INV.Hu.Hu_Tab_IPMaster)
               .LoadContentFrom("IpMaster", "SupplierPrintHu")
               .HtmlAttributes(new { onclick = "CleanTabMessage()" });

         
        })
        .Render();
}

@{ Html.Telerik().ScriptRegistrar()
        .OnDocumentReady(
            @<text>
                var tabIndex = "@TempData["TabIndex"]";
                var tabstrip = $("#TabStrip").data("tTabStrip");
                var item = $("li", tabstrip.element)[tabIndex != "" ? tabIndex : 0];
                tabstrip.select(item);
            </text>
        );
}
