@model com.Sconit.Entity.MD.FinanceCalendar
@{
    ViewBag.Title = "Close";
    Html.RenderAction("_SiteMapPath", "Common", new { MenuContent = "Url_FinanceCalendar_Close" });
}

@using com.Sconit.Web.Models.SearchModels.MD
<fieldset>
    @using (Html.BeginForm("CloseFinanceCalendar", "FinanceCalendar"))
    {   
        <div class="search">
            <div class="search-label">
                <label for="FinanceCalendar_FinanceYear">
                    @Resources.MD.FinanceCalendar.FinanceCalendar_FinanceYear
                </label>
            </div>
            <div class="search-field">
                @Html.DisplayFor(model=>model.FinanceYear)
               
            </div>
             <div class="search-label">
                <label for="FinanceCalendar_FinanceMonth">
                    @Resources.MD.FinanceCalendar.FinanceCalendar_FinanceMonth
                </label>
            </div>
            <div class="search-field">
               @Html.DisplayFor(model => model.FinanceMonth)
            </div>
            <div class="search-label">
                <label for="FinanceCalendar_FinanceYear">
                    @Resources.MD.FinanceCalendar.FinanceCalendar_StartDate
                </label>
            </div>
            <div class="search-field">
               @Html.DisplayFor(model => model.StartDate)
            </div>
             <div class="search-label">
                <label for="FinanceCalendar_FinanceMonth">
                    @Resources.MD.FinanceCalendar.FinanceCalendar_EndDate
                </label>
            </div>
            <div class="search-field">
               @Html.DisplayFor(model => model.EndDate)
            </div>
             

        </div>
        <div class="search-footer">
         @Html.Hidden("isFromList", true)
            <button type="submit" id="btnSearch">
                @Resources.SYS.Global.Button_FinanceCalendar</button>
        </div>

    }
</fieldset>
