@using com.Sconit.Entity.SYS
@model com.Sconit.Entity.SYS.CodeMaster
<fieldset>
    <legend>@Resources.SYS.CodeMaster.Currency_Edit_Title</legend>
    <div class="editor">
        <div class="editor-label-2">
            @Html.LabelFor(model => model.Code)
        </div>
        <div class="editor-field-2">
            @Html.DisplayFor(model => model.Code)
        </div>
        <div class="editor-label-2">
            @Html.LabelFor(model => model.Description)
        </div>
        <div class="editor-field-2">
            @Html.DisplayFor(model => model.Description)
        </div>
    </div>
    <div class="editor-footer">
        <button type="button" id="btnBack" onclick="location.href='@Url.Content("~/CodeMaster/List")'">
            @Resources.SYS.Global.Button_Back</button>
    </div>
    <div>
    </div>
</fieldset>
@{
    Html.RenderAction("_List", "CodeMaster", new { id = Model.Code, type = (int)Model.Type });
}
