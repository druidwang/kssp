@using com.Sconit.Entity.SYS
@model com.Sconit.Entity.Report.CustReportMaster
@{
    Html.RenderAction("_SiteMapPath", "Common", new { MenuContent = "Url_CustReport_ReportMenu" });
}
@using (Html.BeginForm())
{

    <fieldset>
        <legend>@Resources.Report.CustReport.CustReport_New_Title</legend>
        <div class="editor">
            <div class="editor-label-2">
                @Html.LabelFor(model => model.Code)
            </div>
            <div class="editor-field-2">
                @Html.EditorFor(model => model.Code)
            </div>
            <div class="editor-label-2">
                @Html.LabelFor(model => model.Name)
            </div>
            <div class="editor-field-2">
                @Html.EditorFor(model => model.Name)
            </div>
            <div id="importHuid">
                    <table>
                        <tr>
                            <td>
                                <textarea id="Sql" name="Sql" class="normal" rows="20" cols="110">@(Model == null ? "" : Model.Sql)</textarea>
                            </td>
                        </tr>
                    </table>
            </div>
        </div>
        <div class="editor-footer">
            <button type="submit" id="btnNew">
                @Resources.SYS.Global.Button_New</button>
            <button type="button" id="btnBack" onclick="location.href='@Url.Content("~/CustReport/List")'">
                @Resources.SYS.Global.Button_Back</button>
        </div>
    </fieldset>
}